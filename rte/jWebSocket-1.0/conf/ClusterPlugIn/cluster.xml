<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" " http://www.springframework.org/dtd/spring-beans.dtd"> 
<beans>
	<!-- settings  for the mail plug-in -->
	<bean id="org.jwebsocket.plugins.cluster.settings" class="org.jwebsocket.plugins.cluster.Settings">
		<!-- use this broker, e.g. failover:(tcp://0.0.0.0:61616,tcp://127.0.0.1:61616)?initialReconnectDelay=100 -->
		<property name="brokerURI" value=""/>
		<property name="clusterTopic" value="org.jwebsocket.cluster"/>
		<property name="advisoryTopic" value="org.jwebsocket.cluster"/>
		<!-- use id for this node in the cluster -->
		<property name="nodeId" value="n1"/>
		<!-- add these nodes to the cluster -->
		<property name="clusterNodes">
			<list>
				<value>n2</value>
				<value>n3</value>
			</list>
		</property>
	</bean>

</beans>