<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" " http://www.springframework.org/dtd/spring-beans.dtd"> 
<beans>
	<bean id="org.jwebsocket.plugins.scripting.settings" class="org.jwebsocket.plugins.scripting.Settings"
		  init-method="initialize">
		<property name="appsDirectory" value="${JWEBSOCKET_HOME}/apps/js" />
	</bean>

	<bean id="jmsConnectionFactory" class="org.apache.activemq.ActiveMQConnectionFactory">
		<property name="brokerURL">
			<value>tcp://localhost:61616</value>
		</property>
		<!--		
		<property name="userName">
			<value>admin</value>
		</property>
		<property name="password">
			<value>admin</value>
		</property>
		-->
	</bean>
	<bean id="jmsConnection" factory-bean="jmsConnectionFactory" factory-method="createConnection" 
			  init-method="start">
		<property name="clientID" value="jws_server_shared_conn_01" />
	</bean>

</beans>
